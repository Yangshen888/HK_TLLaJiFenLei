<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" tourl="/pages/login/login" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">注册</view></cu-custom><view class="content reg_bg"><view class="list"><view class="list-call"><text class="cuIcon-myfill"></text><input class="biaoti" maxlength="11" placeholder="用户" data-event-opts="{{[['input',[['__set_model',['$0','loginName','$event',[]],['userinfo']]]]]}}" value="{{userinfo.loginName}}" bindinput="__e"/></view><view class="list-call"><text class="cuIcon-unlock"></text><input class="biaoti" type="text" maxlength="32" placeholder="登录密码" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['$0','passWord','$event',[]],['userinfo']]]]]}}" value="{{userinfo.passWord}}" bindinput="__e"/><image class="img" src="{{showPassword?'/static/shilu-login/op.png':'/static/shilu-login/cl.png'}}" data-event-opts="{{[['tap',[['display',['$event']]]]]}}" bindtap="__e"></image></view><view class="list-call"><text class="cuIcon-lock"></text><input class="biaoti" type="text" maxlength="32" placeholder="确认登录密码" password="{{!showPassword2}}" data-event-opts="{{[['input',[['__set_model',['$0','passWord2','$event',[]],['userinfo']]]]]}}" value="{{userinfo.passWord2}}" bindinput="__e"/><image class="img" src="{{showPassword2?'/static/shilu-login/op.png':'/static/shilu-login/cl.png'}}" data-event-opts="{{[['tap',[['display2',['$event']]]]]}}" bindtap="__e"></image></view><view class="list-call"><text class="cuIcon-home"></text><pick-regions style="position:absolute;left:65px;" vue-id="2" default-regions="{{defaultRegions}}" data-event-opts="{{[['^getRegions',[['handleGetRegions']]]]}}" bind:getRegions="__e" bind:__l="__l" vue-slots="{{['default']}}"><input placeholder="选择省市区" disabled data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['userinfo']]]]]}}" value="{{userinfo.address}}" bindinput="__e"/></pick-regions></view><view class="list-call"><text class="cuIcon-vipcard"></text><input class="biaoti" type="number" maxlength="11" placeholder="老年卡号" data-event-opts="{{[['input',[['__set_model',['$0','oldCard','$event',[]],['userinfo']]]]]}}" value="{{userinfo.oldCard}}" bindinput="__e"/></view><view class="list-call"><text class="cuIcon-mobilefill"></text><input class="biaoti" type="number" maxlength="11" placeholder="手机" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['userinfo']]]]]}}" value="{{userinfo.phone}}" bindinput="__e"/></view></view><view class="dlbutton" hover-class="dlbutton-hover" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" bindtap="__e"><text>注册</text></view><view class="xieyi"><image src="{{xieyi==true?'/static/shilu-login/ty1.png':'/static/shilu-login/ty0.png'}}" data-event-opts="{{[['tap',[['xieyitong',['$event']]]]]}}" bindtap="__e"></image><text data-event-opts="{{[['tap',[['xieyitong',['$event']]]]]}}" bindtap="__e">同意</text><navigator url="blog?id=1" open-type="navigate">《软件用户协议》</navigator></view></view></view>
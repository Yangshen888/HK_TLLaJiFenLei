<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" tourl="/pages/home/index" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">赋分记录</view></cu-custom><form><view class="cu-form-group pa"><view class="title">请选择年月日</view><picker mode="date" value="{{query.startdate}}" data-event-opts="{{[['change',[['StartDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{query.startdate}}</view></picker></view><view class="pa"><t-table style="background:#fff;" bind:change="__e" vue-id="2" data-event-opts="{{[['^change',[['change']]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><t-tr vue-id="{{('3')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-th style="color:rgb(46, 100, 233);" vue-id="{{('4')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">序号</t-th><t-th style="color:rgb(46, 100, 233);" vue-id="{{('5')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">用户</t-th><t-th style="color:rgb(46, 100, 233);" vue-id="{{('6')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">获得积分</t-th><t-th style="color:rgb(46, 100, 233);" vue-id="{{('7')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">赋分时间</t-th><t-th style="color:rgb(46, 100, 233);" vue-id="{{('8')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">垃圾厢房编号</t-th><t-th style="color:rgb(46, 100, 233);" vue-id="{{('9')+','+('3')}}" bind:__l="__l" vue-slots="{{['default']}}">操作人</t-th></t-tr><block wx:for="{{tableList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><t-tr vue-id="{{('10-'+index)+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-td vue-id="{{('11-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{index+1}}</t-td><t-td vue-id="{{('12-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.realName}}</t-td><t-td vue-id="{{('13-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.score}}</t-td><t-td vue-id="{{('14-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.addTime}}</t-td><t-td vue-id="{{('15-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.grabageRoomNum}}</t-td><t-td vue-id="{{('16-'+index)+','+('10-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.addPeople}}</t-td></t-tr></block></t-table></view></form></view>
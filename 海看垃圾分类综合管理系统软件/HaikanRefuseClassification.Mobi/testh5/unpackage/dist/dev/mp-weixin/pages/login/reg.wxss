}
.dlbutton {
	color: #FFFFFF;
	font-size: 34rpx;
	width:470rpx;
	height:100rpx;
	background:-webkit-gradient(linear,right top, left top,from(rgba(63,205,235,1)),to(rgba(188,226,158,1)));
	background:-webkit-linear-gradient(right,rgba(63,205,235,1),rgba(188,226,158,1));
	background:linear-gradient(-90deg,rgba(63,205,235,1),rgba(188,226,158,1));
	-webkit-box-shadow:0rpx 0rpx 13rpx 0rpx rgba(164,217,228,0.2);
	        box-shadow:0rpx 0rpx 13rpx 0rpx rgba(164,217,228,0.2);
	-webkit-border-radius:50rpx;
	        border-radius:50rpx;
	line-height: 100rpx;
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	/* margin-top: 100upx; */
}
.dlbutton-hover {
	background:-webkit-gradient(linear,right top, left top,from(rgba(63,205,235,0.9)),to(rgba(188,226,158,0.9)));
	background:-webkit-linear-gradient(right,rgba(63,205,235,0.9),rgba(188,226,158,0.9));
	background:linear-gradient(-90deg,rgba(63,205,235,0.9),rgba(188,226,158,0.9));
}

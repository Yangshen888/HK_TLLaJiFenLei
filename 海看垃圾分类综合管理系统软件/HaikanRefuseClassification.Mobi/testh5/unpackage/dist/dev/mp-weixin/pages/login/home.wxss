
.tugurl {
	color: #999;
}
.sharbuttn {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.shartitle {
	width: 80%;
	text-align: center;
	margin-left: 10%;
	border-bottom: 1px solid #dddddd;
	position: relative;
	height: 60rpx;
}
.tgtit {
	text-align: center;
}
.shartitle view {
	height: 50rpx;
	line-height: 50rpx;
	font-size: 28rpx;
	color: #999;
	width: 120rpx;
	margin: 32rpx auto;
	position: absolute;
	background: #fff;
	left: 50%;
	margin-left: -60rpx;
}
.sharapk {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin: 20rpx auto;
}
.sharapk view {
	margin: 40rpx;
}
.sharapk view image {
	height: 80rpx;
	width: 80rpx;
}
.content {
	width: 100%;
	/* background-color: #ffffff; */
}
.top {
	width: 100%;
	height: 400rpx;
	/* background: url(http://pds.jyt123.com/wxtest/banner.png) no-repeat ; */
	background-size: 100%;
	background-position: center center;
}
.banner {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	width: 100%;
	height: 100%;
	background-color: #ffffff;
	/* background-image: url(../../static/image/index.png); */
	/* border-radius: 30px 30px 0 0;
		    margin-top: -30px; */
	position: absolute;
	/* top: 280px; */
	bottom: 0;
}
.banner ._dl {
	margin-top: -80rpx;
}
.banner ._dl ._dt {
	text-align: center;
}
.banner ._dl ._dt image {
	width: 160rpx;
	height: 160rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.banner ._dl ._dd {
	text-align: center;
}
.img {
	width: 400rpx;
	height: 400rpx;
	/* background-color: red; */
	margin: 0 auto;
	margin-top: 60rpx;
	text-align: center;
}
.img image {
	width: 100%;
	height: 100%;
}
.btn {
	width: 260rpx;
	height: 60rpx;
	line-height: 60rpx;
	margin: 0 auto;
	margin-top: 60rpx;
	-webkit-border-radius: 40rpx;
	        border-radius: 40rpx;
	border: 0;
	font-size: 26rpx;
	outline: 0;
	background: #33b17b;
	color: #ffffff;
	text-align: center;
}
.bottom {
	width: 100%;
	height: 400rpx;
	/* 	background: url(../../static/img/beij.png) no-repeat; */
	background-position: left bottom; /* 设置背景图片位置 */
	background-size: 20%;
}
.bottom ._ul {
	padding-left: 40rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.bottom ._ul ._li {
	width: 100%;
	height: 60rpx;
}
.btn-wrap {
	position: fixed;
	right: 30rpx;
	top: 8vh;
}

